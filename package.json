{
  "name": "psybudget",
  "private": false,
  "version": "0.0.0",
  "license": "MIT",
  "author": "Psycarlo <psycarlo1@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --config --check .prettierrc src",
    "format:fix": "prettier --config .prettierrc src --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,vue,json}": [
      "yarn lint:fix",
      "yarn format:fix"
    ]
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
    "pinia": "^2.0.16",
    "vee-validate": "^4.6.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^28.1.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "9.0.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "eslint": "8.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^28.0.6",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.38.4",
    "vue3-jest": "^27.0.0-alpha.2"
  }
}
